/dts-v1/;

#include "mt7621.dtsi"
#include <dt-bindings/input/input.h>
#include "mt7621-raeth-gmac2.dtsi"
#include "mt7621-pinctrl-default-gpio.dtsi"

/ {
	compatible = "mediatek,mt7621-soc";
	model = "Linksys-EA7500";

	chosen {
		bootargs = "console=ttyS0,115200";
	};

	leds {
		compatible = "gpio-leds";

		power {
			label = "power";
			gpios = <&gpio0 10 GPIO_ACTIVE_HIGH>;
		};

		wlan {
			label = "wlan";
			gpios = <&gpio0 5 GPIO_ACTIVE_HIGH>;
		};
	};

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

    	        wps {
			label = "wps";
			gpios = <&gpio0 17 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};

		reset {
			label = "reset";
			gpios = <&gpio0 12 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};
};

&bch {
	status = "okay";
};

&nand {
	status = "okay";

	flash@0 {
		reg = <0>;
		nand-ecc-mode = "hw";
		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "Bootloader";
				reg = <0x00000 0x80000>; /* 512k */
			};

			partition@80000 {
				label = "CFG_Bdata";
				reg = <0x80000 0x80000>; /* 512k */
			};

			partition@100000 {
				label = "Factory";
				reg = <0x100000 0x80000>; /* 512k */
			};

			partition@180000 {
				label = "Crash";
				reg = <0x180000 0x80000>; /* 512k */
			};

			partition@200000 {
				label = "firmware";
				reg = <0x200000 0x2000000>; /* 32M */
			};

			partition@2200000 {
				label = "Config";
				reg = <0x2200000 0x80000>; /* 512K */
			};

			partition@2280000 {
				label = "Storage";
				reg = <0x2280000 0x400000>; /* 4096K */
			};

			partition@2680000 {
				label = "RWFS";
				reg = <0x2680000 0x5900000>;
			};

			partition@all {
				label = "ALL";
				reg = <0x0 0x7f80000>;
			};
		};
	};
};

&pcie {
	status = "okay";
};

&xhci {
	status = "okay";
};

&u3phy1 {
	status = "okay";
};
